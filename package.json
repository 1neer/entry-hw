{
  "name": "Entry_HW",
  "scripts": {
    "clean": "rimraf dist",
    "pack": "build",
    "dist": "build --arch ia32",
    "start": "set NODE_ENV=development&&electron -d app"
  },
  "build": {
    "dmg": {
      "icon": "build/icon.icns",
      "iconSize": 80,
      "background": "build/background.png",
      "contents": [
        {
          "x": 240,
          "y": 120
        },
        {
          "x": 380,
          "y": 250,
          "type": "file",
          "path": "./app/README.md"
        },
        {
          "x": 240,
          "y": 380,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "loadingGif": "build/install-spinner.gif",
      "title": "Entry-HW",
      "icon": "./build/icon.ico",
      "authors": "EntryLabs"
    },
    "asar": true,
    "appId": "entry-hw",
    "productName": "Entry-HW",
    "extend-info": "build/build.plist",
    "iconUrl": "https://raw.githubusercontent.com/entrylabs/entry-hw/feature/electron/build/icon.ico"
  },
  "devDependencies": {
    "electron-builder": "^7.10.2",
    "electron-packager": "^8.1.0",
    "electron-prebuilt": "1.4.5",
    "rimraf": "^2.5.4"
  },
  "dependencies": {}
}
